{
  "env": {
    "browser": true,
    "commonjs": true,
    "es2021": true
  },
  "extends": "airbnb-base",
  "parserOptions": {
    "ecmaVersion": "latest"
  },
  "rules": {
    "no-console": "off",
    "quotes": [2, "double", { "avoidEscape": true }],
    "comma-dangle": "off",
    "no-unexpected-multiline": "off"
  }
}
